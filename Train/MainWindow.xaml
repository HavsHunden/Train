<Window x:Class="Train.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Train"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Canvas Name="MainCanvas" Background="Yellow" ContextMenuOpening="MainCanvas_ContextMenuOpening" ContextMenuClosing="MainCanvas_ContextMenuClosing">
        <Canvas.ContextMenu>
            <ContextMenu Name ="MainContextMenu">
                <MenuItem Header="Add week" Click="MenuItem_Click">
                    
                </MenuItem>
            </ContextMenu>
        </Canvas.ContextMenu>
        
        <DockPanel Name="MainDock" LastChildFill="False" Background="Aquamarine">
            <Label Content="Label" Name ="overWhat"/>


        </DockPanel>
        
    </Canvas>

</Window>
